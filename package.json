    "babel-preset-gatsby": "0.2.20",
    "browserslist": "4.16.5",
    "concurrently": "5.0.0",
    "eslint": "6.6.0",
    "eslint-config-airbnb-base": "13.2.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jest": "22.21.0",
